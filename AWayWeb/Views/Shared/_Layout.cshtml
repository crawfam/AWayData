<!--jquery-classybox LightBox--> 
<script>

        $(".lightbox span.zoom a").ClassyBox({
        widthWindow:  900
        });

        // datepicker
        $('#txtDatePicker').datepicker({
            startDate: "01/01/2006",
            endDate: "@DateTime.Now.ToShortDateString()"            
        });

        function getDay() {
            var urlDay = $('#txtDatePicker').val();
            if (urlDay != 'Date') {
                $('#txtDatePicker').val(urlDay);                
                window.location = '@Url.Action("Day", "Home")/?date=' + urlDay;
            }
        }

</script>

